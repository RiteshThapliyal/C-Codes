// Method 1: Simple Code

#include <stdio.h>

int main() {
    // Write C code here
    int first = printf("Enter First Number: ");
    scanf("%d", &first);

    int second = printf("Enter Second Number: ");
    scanf("%d", &second);

    printf("Multiplication of Entered Two Numbers: %d", first * second);

    return 0;
}


// Method 2: By using Function

#include <stdio.h>

void Multiplication();  // prototype declaration
int main() {
    // Write C code here
    Multiplication();

    return 0;
}

void Multiplication(){
    int first = printf("Enter first value: ");
    scanf("%d", &first);
    int second = printf("Enter second value: ");
    scanf("%d", &second);
    int result = first * second;
    printf("Multiplication of given number is: %d", result);
}
